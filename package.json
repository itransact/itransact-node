{
  "name": "itransact-node",
  "version": "2.0.0",
  "description": "This is an SDK for authenticating with iTransact's API",
  "main": "itransact-core.js",
  "scripts": {
    "test": "node node_modules/nyc/bin/nyc node_modules/mocha/bin/mocha \"test/**/*.js\"",
    "test-report": "node node_modules/nyc/bin/nyc report",
    "test-check-coverage": "node node_modules/nyc/bin/nyc check-coverage --lines 95 --functions 95 --branches 95",
    "lint": "npx eslint examples/**.js; npx eslint models/**.js; npx eslint test/**.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itransact/itransact-node.git"
  },
  "keywords": [
    "itransact",
    "payroc",
    "payment",
    "merchant",
    "transaction",
    "integration",
    "credit",
    "card",
    "check",
    "ach",
    "omnichannel"
  ],
  "author": "Preston Farr <preston.farr@payroc.com> (http://www.itransact.com)",
  "license": "MIT",
  "runkitExample": "var itransactNode = require(\"itransact-node\");",
  "bugs": {
    "url": "https://github.com/itransact/itransact-node/issues"
  },
  "homepage": "https://github.com/itransact/itransact-node#readme",
  "devDependencies": {
    "chai": "^4.1.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^6.1.3",
    "nyc": "^14.0.0"
  },
  "dependencies": {
    "request": "^2.88.0"
  }
}
